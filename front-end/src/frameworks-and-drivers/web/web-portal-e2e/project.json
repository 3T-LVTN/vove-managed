{
  "name": "web-portal-e2e",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src/frameworks-and-drivers/web/web-portal-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "src/frameworks-and-drivers/web/web-portal-e2e/cypress.config.ts",
        "devServerTarget": "web-portal:serve:development",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "web-portal:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "src/frameworks-and-drivers/web/web-portal-e2e/**/*.{js,ts}"
        ]
      }
    }
  },
  "tags": ["layer:frameworks-and-drivers"],
  "implicitDependencies": ["web-portal"]
}

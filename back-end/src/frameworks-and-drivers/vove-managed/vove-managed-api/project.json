{
  "name": "vove-managed-api",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src/frameworks-and-drivers/vove-managed/vove-managed-api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/src/frameworks-and-drivers/vove-managed/vove-managed-api",
        "main": "src/frameworks-and-drivers/vove-managed/vove-managed-api/src/main.ts",
        "tsConfig": "src/frameworks-and-drivers/vove-managed/vove-managed-api/tsconfig.app.json",
        "assets": [
          "src/frameworks-and-drivers/vove-managed/vove-managed-api/src/assets"
        ],
        "isolatedConfig": true,
        "webpackConfig": "src/frameworks-and-drivers/vove-managed/vove-managed-api/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "vove-managed-api:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "vove-managed-api:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "src/frameworks-and-drivers/vove-managed/vove-managed-api/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "src/frameworks-and-drivers/vove-managed/vove-managed-api/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": ["layer:frameworks-and-drivers"]
}
